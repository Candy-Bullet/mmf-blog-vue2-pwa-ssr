    "morgan": "^1.9.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "request-promise": "^4.2.4",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.5.0",
    "shelljs": "^0.8.3",
