{
  "name": "mmf-blog-vue2-pwa-ssr",
  "version": "1.0.0",
  "description": "这是一个 pwa 项目",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "scripts": {
    "dev": "npm run static && cross-env MICRO_CACHE=false node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "npm run static && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
    "static": "node ./build/copy",
    "lint:client": "eslint --ext .jsx,.js,.vue src/ --fix",
    "lint:server": "eslint --ext .js server/ --fix",
    "lint": "npm run lint:client && npm run lint:server",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "fastclick": "^1.0.6",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "marked": "^0.5.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "raven-js": "^3.26.4",
    "store2": "^2.7.0",
    "toastr": "^2.1.4",
    "vue": "^2.5.17",
    "vue-meta": "^1.5.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/runtime": "^7.0.0-rc.1",
    "autoprefixer": "^9.1.1",
    "babel-loader": "^8.0.0-beta.3",
    "body-parser": "^1.18.3",
    "cache-loader": "^1.2.2",
    "compression": "^1.7.3",
    "connect-multiparty": "^2.1.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint-config-lcy-vue": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^0.14.3",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "lru-cache": "^4.1.3",
    "mini-css-extract-plugin": "^0.4.1",
    "mongoose": "5.2.8",
    "morgan": "^1.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.5.0",
    "shelljs": "^0.8.2",
    "sw-precache": "^5.1.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "thread-loader": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.0",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 0.1%",
    "last 3 versions",
    "not ie <= 8",
    "iOS > 8"
  ],
  "lint-staged": {
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  }
}
