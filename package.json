{
  "name": "mmf-blog-vue2-pwa-ssr",
  "version": "1.0.0",
  "description": "这是一个 pwa 项目",
  "author": "lincenying <lincenying@qq.com>",
  "private": true,
  "scripts": {
    "dev": "yarn static && cross-env MICRO_CACHE=false node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "yarn static && yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
    "static": "node ./build/copy",
    "lint:client": "eslint --ext .jsx,.js,.vue src/ --fix",
    "lint:server": "eslint --ext .js server/ --fix",
    "lint": "yarn lint:client && yarn lint:server"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "fastclick": "^1.0.6",
    "highlight.js": "^9.15.8",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.6.2",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "raven-js": "^3.27.2",
    "store2": "^2.7.1",
    "toastr": "^2.1.4",
    "vue": "^2.6.10",
    "vue-content-loader": "^0.2.1",
    "vue-meta": "^1.6.0",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cache-loader": "^4.0.0",
    "compression": "^1.7.4",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint-config-lcy-vue": "^3.0.5",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^2.4.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
