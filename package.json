    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.2.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 0.1%",
    "last 3 versions",
    "not ie <= 8",
    "iOS > 8"
  ],
  "lint-staged": {
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
